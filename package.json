{
  "name": "nestjs-formdata-interceptor",
  "version": "1.5.1",
  "description": "nest js formdata interceptor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "h3llmy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h3llmy/nest-formdata-interceptor.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "development",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "test": "jest --detectOpenHandles",
    "prepublish": "npm test && npm run build",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/*.spec.ts",
      "**/*.test.ts"
    ]
  },
  "exports": {
    ".": "./dist/index.js",
    "./s3FileSaver": "./dist/fileSaver/s3.file-saver.js",
    "./defaultFileSaver": "./dist/fileSaver/default.file-saver.js",
    "./localFileSaver": "./dist/fileSaver/local.file-saver.js",
    "./validator": "./dist/validators/index.js"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.955.0",
    "@types/busboy": "^1.5.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "class-validator": "^0.14.3",
    "jest": "^30.2.0",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "busboy": "^1.6.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.x",
    "@nestjs/common": "^11.x",
    "class-validator": "^0.x",
    "rxjs": "^7.x"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "class-validator": {
      "optional": true
    }
  },
  "keywords": [
    "nestjs",
    "formdata",
    "interceptor",
    "form data",
    "nestjs multipart",
    "nestjs form data",
    "nestjs form data interceptor",
    "nestjs file upload",
    "nestjs validate file",
    "nestjs store file",
    "file middleware",
    "nestjs multipart/form-data"
  ]
}
