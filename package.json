{
  "name": "nestjs-formdata-interceptor",
  "version": "1.3.4",
  "description": "nest js formdata interceptor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": {
    "name": "h3llmy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h3llmy/nest-formdata-interceptor.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "development",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "test": "jest",
    "prepublish": "npm test && npm run build",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/*.spec.ts",
      "**/*.test.ts"
    ]
  },
  "devDependencies": {
    "@types/busboy": "^1.5.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "jest": "^29.7.0",
    "semantic-release": "^24.2.3",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "busboy": "^1.6.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.x",
    "class-validator": "^0.x",
    "rxjs": "^7.x"
  },
  "optionalDependencies": {
    "@aws-sdk/client-s3": "^3.x"
  },
  "keywords": [
    "nestjs",
    "formdata",
    "interceptor",
    "form data",
    "nestjs multipart",
    "nestjs form data",
    "nestjs form data interceptor",
    "nestjs file upload",
    "nestjs validate file",
    "nestjs store file",
    "file middleware",
    "nestjs multipart/form-data"
  ]
}
